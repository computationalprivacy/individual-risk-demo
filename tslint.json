{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier", "@sentry/typescript/tslint"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "no-unused-expression": false,
    "ordered-imports": false,
    "no-unused-variable": false
  },
  "defaultSeverity": "warning"
}
